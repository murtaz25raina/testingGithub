    agent any
    stages{
        stage("Hello World"){
            steps{echo "Hello World" }
        }
                stage("Hi"){
            steps{echo "Hi" }
        }
                stage("Murtaz"){
            steps{echo "Murtaz" }
        }
                stage("Raina"){
            echo{git 'Raina'}
        }
    }
}
